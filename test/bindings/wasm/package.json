{
  "name": "in3-wasm",
  "version": "2.0.0-beta.5",
  "description": "Typescript-version of the incubed client",
  "main": "js/src/index.js",
  "scripts": {
    "build": "tsc -p .",
    "test": "mocha --timeout 5000 --full-trace  *.js",
    "test_api": "mocha --timeout 5000 --full-trace  testApi.js"
  },
  "nyc": {
    "include": [
      "*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "Simon Jentzsch",
  "license": "(AGPL-3.0-or-later OR commercial)",
  "bugs": {
    "url": "https://github.com/slockit/in3-c/issues"
  },
  "homepage": "https://github.com/slockit/in3-c#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "chai": "^4.1.2",
    "handlebars": ">=4.1.2",
    "mocha": "^5.1.1",
    "nyc": "^11.8.0",
    "source-map-support": "^0.5.6"
  },
  "dependencies": {
    "bn.js": "^5.0.0",
    "buffer": "^5.2.1",
    "in3-common": "^2.0.3-RC1"
  },
  "keywords": [
    "ethereum",
    "slockit",
    "iot",
    "in3",
    "client",
    "jsonrpc"
  ]
}